# stock fstab - you probably want to override this with a machine specific one

/dev/root            /                    auto       ro              1  0
proc                 /proc                proc       defaults              0  0
devpts               /dev/pts             devpts     mode=0620,ptmxmode=0666,gid=5      0  0

/dev/hda4            /home                ext4       defaults,noatime       0  0

/dev/aosvg/downloads /var/aos/downloads   ext4       defaults,nofail,noatime,x-systemd.device-timeout=5s  0  0
/dev/aosvg/workdirs  /var/aos/workdirs    ext4       defaults,nofail,noatime,x-systemd.device-timeout=5s  0  0
/dev/aosvg/storages  /var/aos/storages    ext4       defaults,nofail,noatime,usrjquota=aquota.user,jqfmt=vfsv0,x-systemd.device-timeout=5s  0  0
/dev/aosvg/states    /var/aos/states      ext4       defaults,nofail,noatime,usrjquota=aquota.user,jqfmt=vfsv0,x-systemd.device-timeout=5s  0  0
